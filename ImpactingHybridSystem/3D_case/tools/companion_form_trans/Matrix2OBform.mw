#(*
OB_form :=proc(A,C)
	local s,i,j,temp,z,OC,OZ,OB_form;
	OC:= simplify(C);	
	s:=size(A);
	temp :=IdentityMatrix(s(1));
	for i from 1 by 1 to s(1)-1 do
	temp:=simplify(temp.Transpose(A));
	OC:=simplify(&lt;temp.C|OC&gt;);
	end do;
	z := simplify(MatrixInverse(Transpose(OC)).C);
	OZ := z;
	temp :=IdentityMatrix(s(1));
	for j from 1 by 1 to s(1)-1 do
	temp:=simplify(temp.A);
	OZ:=simplify(&lt;temp.z|OZ&gt;);
	end do;
	OB_form := OZ;
	end proc:
#*)</EC-CodeEditor></CodeEditor-ExecGroup></Presentation-Block><Presentation-Block>
